void get_new_user(void){
	int j,k;
	int  i,empty;
	char id[7];
	
	for(j=0; j<n ;j++){

	for(i=0; i<n; i++){
		
		if(strlen(student[i].student_id) == 0 ){
		
		} 
		
	}
	Retake:
	
	printf("Enter User ID: ");
	scanf("%s",id);
	
		for(k=0; k<n;k++){
			if(strcmp(id, student[k].student_id)!=0){
				printf("The User already exsist, plz enter another id");
				goto Retake; 
			}
			else{
				strcpy(id,student[i].student_id);
				
			}
		}
	
	printf("Enter User name: ");
	scanf("%s",student[i].name);
	printf("Enter Mid 1 marks of %s: ",student[i].name);
	scanf("%d",&student[i].mid1);
	printf("Enter Mid 2 Marks of %s: ",student[i].name);
	scanf("%d",&student[i].mid2);
	printf("Enter Quiz Marks of %s: ",student[i].name);
	scanf("%d",&student[i].quiz);
	printf("Enter Assigment Marks of %s: ",student[i].name);
	scanf("%d",&student[i].assigment);	
	printf("Enter Finals Marks of %s: ",student[i].name);
	scanf("%d",&student[i].final);	
			}
